<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>0.4.0</Version>
    <Authors>Yulsn ApS</Authors>
    <Company>Yulsn ApS</Company>
    <Product>Api Client for Yulsn</Product>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>.net client for Yulsn api</Description>
    <RepositoryUrl>https://github.com/Yulsn/YulsnApiClient</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Yulsn/YulsnApiClient</PackageProjectUrl>    
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="8.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="8.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
